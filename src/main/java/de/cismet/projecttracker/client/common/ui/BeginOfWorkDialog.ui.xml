<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.input-component {
			width: 100%;
		}
                
		.input-component-left {
			float:left;
                        width:440px;
		}
		.input-component-margin {
			margin-left: 20px;
		}
	</ui:style>
	<g:HTMLPanel>
		<div
			style="position: relative; top: auto; left: auto; margin: 0 auto; z-index: 1"
			class="modal">
			<div class="modal-header">
        <h3>Missing Begin of Work Activity</h3>
				<!--a class="close" href="">Ã—</a-->
			</div>
			<div class="modal-body">
                            <div>There is no Begin of Work Activity for today. Would you like to create one?</div>
			</div>
			<div class="modal-footer">
				<g:Button styleName="btn primary" text="OK" ui:field="okButton" />
				<g:Button styleName="btn primary" text="Cancel" ui:field="closeButton" />
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>